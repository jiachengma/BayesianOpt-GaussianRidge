within DynamicVCC.Utilities;
function humidityRatio_pT "Return saturated humidity ratio of moist air as a function of pressure and temperature"
  extends Modelica.Icons.Function;

  input SI.AbsolutePressure p;
  input SI.Temperature T;
  output SI.MassFraction x_sat;

algorithm
  x_sat:=9.0378167615928758001106975309732/(exp(0.0000088241638508146716390889707781753*p - 0.052799219434387653946158833504241*T + 20.809608788943785872498853311501) + 1.0) + 0.002108465290584325852798312335987/(exp(0.099624184438616798015825932250462*T - 0.0000409648190084039187652820146468*p - 22.454402243180972763446511129991) + 1.0) -
    2.3896378997089025576133405893104/(exp(0.10858369581602044857188755284167*T - 0.000053264312879703756831162309026695*p - 36.075228357100244879084193093553) + 1.0) + 2.3863902786089127030572411209633;

end humidityRatio_pT;
